<?php

$seteMaravilhas=[
    "Cristo Redentor",
    "Chichén Itzá",
    "Coliseu",
    "Muralha da China",
    "Petra",
    "Machu Pichu",
    "Taj Mahal"
];

for($i=0;$i<7;$i++){
    print($i+1 . ":" . $seteMaravilhas[$i] . "\n");
}

